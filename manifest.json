{
  "manifest_version": 2,
  "name": "qiitadon display filename",
  "description": "This extension display filename for qiitadon code syntax.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://qiitadon.com/",
        "https://qiitadon.com/*"
      ],
      "js": ["main.js"],
      "css": ["main.css"]
    }
  ]
}